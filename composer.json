{
  "name": "andmemasin/yii2-helpers",
  "type": "library",
  "description": "general yii2 helper classes",
  "keywords": [
    "helpers",
    "andmemasin"
  ],
  "authors": [
    {
      "name": "TÃµnis Ormisson",
      "email": "tonis@andmemasin.eu"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.0.14",
    "ramsey/uuid": "^3 | ^4"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/specify": "~0.4.6",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "codeception/module-filesystem": "^1.0.0",
    "codeception/module-yii2": "^1.1.0",
    "codeception/module-asserts": "^1.3.0",
    "codeception/module-db": "^1.0"
  },

  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "autoload": {
    "psr-4": {
      "andmemasin\\helpers\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.4.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
